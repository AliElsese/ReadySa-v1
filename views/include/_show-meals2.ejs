<% for(var i = 0; i < meals.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= meals[i].mealname %></td>
        <td><%= meals[i].mealprice %><sub>RS</sub></td>
        <td><%= meals[i].mealdescription %></td>
        <td><%= meals[i].mealpoints %></td>
        <td><%= meals[i].mealtime %></td>
        <td>
            <img src="/<%= meals[i].mealimage %>" style="width: 100px;height: 100px;" alt="">
        </td>
        <td>
            <a href="/api-deleteMealRestaurant/<%= meals[i].restaurantid %>/<%= meals[i].mealid %>" style="display: inline-block; color: darkred;" class="delete"> 
                حذف 
            </a>
            <br>
            <a href="/update-mealRestaurant/<%= meals[i].mealid %>" style="display: inline-block; color: blue;" class="delete"> 
                تعديل 
            </a>
        </td>
    </tr>
<% } %>